" ------------------------------------------------------------------------------
"  My .givimrc


" ------------------------------------------------------------------------------
" 文字化け回避
source $VIMRUNTIME/delmenu.vim
set langmenu=ja_jp.utf-8
source $VIMRUNTIME/menu.vim

" Font
set guifont=Sauce_Code_Powerline:h14

" カラースキーム
colorscheme wombat256mod

" 起動時のウィンドウサイズ
set columns=110
set lines=40

" クリップボード共有
set clipboard=unnamed

